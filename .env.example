# Environment Variables for MyMindSpace Semantic Search Service
# Copy this file to .env and update with your actual values

# AstraDB Configuration (Required)
# Get these from your AstraDB console: https://astra.datastax.com
ASTRA_DB_ID=your_database_id_here
ASTRA_DB_REGION=us-east1
ASTRA_DB_KEYSPACE=default_keyspace
ASTRA_DB_APPLICATION_TOKEN=your_application_token_here
ASTRA_DB_API_ENDPOINT=https://your_db_id-your_region.apps.astra.datastax.com

# Server Configuration
NODE_ENV=development
PORT=3000

# Security - Generate a strong API key for production
# Example: openssl rand -hex 32
API_KEY=your_secure_api_key_here

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Vector Configuration
DEFAULT_VECTOR_DIMENSIONS=90
MAX_VECTOR_DIMENSIONS=4096

# Logging Configuration
LOG_LEVEL=info
